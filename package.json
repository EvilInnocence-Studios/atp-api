{
  "name": "api.evilinnocence.com",
  "version": "0.0.1",
  "description": "An API for the once upon a time site",
  "main": "index.js",
  "scripts": {
    "init-db": "clear && ts-node ./src/init-db.ts",
    "copy-env": "cp ./.env ./dist/.env",
    "build": "rm -rf dist && tsc && yarn copy-env && yarn copy-package && yarn dist-install",
    "copy-package": "cp ./package.json ./dist/package.json",
    "dist-install": "cd ./dist && yarn install --prod",
    "deploy": "ts-node ./deploy.ts",
    "full-deploy": "yarn build && yarn deploy",
    "start": "node ./dist/index.js",
    "ts-start": "ts-node ./src/core/index.ts",
    "dev": "yarn copy-env && tsc-watch --onSuccess \"node ./dist/index.js\"",
    "profile": "node -r ts-node/register --prof ./index.ts",
    "status": "node ./src/core-shared/scripts/status.mjs",
    "install-custom": "node ./scripts/install-custom.js",
    "password": "ts-node ./src/generate-password-hash.ts",
    "redirects": "node ./scripts/build-redirects.js",
    "webpack": "webpack --config ./webpack.config.js",
    "deploy-server": "rsync -avz -e 'ssh -i ~/credentials/account-info/aws-ubuntu/EvilInnocence.pem' ./deploy/*.js ubuntu@testbed.wittrock.us:/var/www/api.evilinnocence.com",
    "full-deploy-server": "clear && yarn webpack && yarn deploy-server"
  },
  "author": "Andrew Wittrock",
  "license": "UNLICENSED",
  "devDependencies": {
    "@aws-sdk/client-api-gateway": "^3.758.0",
    "@aws-sdk/client-cloudfront-keyvaluestore": "^3.758.0",
    "@aws-sdk/client-lambda": "^3.758.0",
    "@aws-sdk/signature-v4-crt": "^3.758.0",
    "@types/archiver": "^6.0.3",
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.2",
    "@types/superagent": "^8.1.9",
    "archiver": "^7.0.1",
    "aws-sdk": "^2.1692.0",
    "babel-loader": "^9.2.1",
    "chalk": "^5.3.0",
    "csv-parser": "^3.2.0",
    "fast-xml-parser": "^4.5.1",
    "node-html-markdown": "^1.3.0",
    "null-loader": "^4.0.1",
    "table": "^6.9.0",
    "throttled-queue": "^2.1.4",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@aws-sdk/credential-providers": "^3.758.0",
    "@aws-sdk/client-cloudfront": "^3.758.0",
    "@aws-sdk/client-s3": "^3.717.0",
    "@aws-sdk/client-ses": "^3.723.0",
    "@aws-sdk/s3-request-presigner": "^3.717.0",
    "@babel/core": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@paypal/paypal-server-sdk": "^0.6.1",
    "@react-email/components": "^0.0.31",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "esm-hook": "^0.1.4",
    "express": "^5.0.1",
    "express-fileupload": "^1.5.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^3.1.0",
    "pg": "^8.7.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "serverless-api-boilerplate": "^0.4.2",
    "serverless-http": "^3.2.0",
    "superagent": "^10.1.1",
    "ts-functional": "^1.22.0"
  }
}
